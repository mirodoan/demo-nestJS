{
  "family": "miro-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "miroapp",
      "image": "983101357771.dkr.ecr.us-east-1.amazonaws.com/miroapp:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "name": "miroapp-3000-tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "JWT_SECRET",
          "value": "supersecret"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgres://postgres:Timo9356@database-1.c6jgg2maa55n.us-east-1.rds.amazonaws.com:5432/postgres"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "readonlyRootFilesystem": false,
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/miro-task",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::983101357771:role/ecsTaskExecutionRole",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
